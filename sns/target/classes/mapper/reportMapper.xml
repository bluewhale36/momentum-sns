<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IF_ReportDAO">

	<insert id="report" parameterType="vo.ReportVO">
	<choose>
		<when test='st_type== "curse"'>
			insert into reporttest values (#{no}, #{id}, #{reportId}, default, #{r_loc}, #{st_type}, '욕설', #{r_cont})
		</when>
		<otherwise>
			insert into reporttest values (#{no}, #{id}, #{reportId}, default, #{r_loc}, #{st_type}, #{nd_type}, #{r_cont})
		</otherwise>
	</choose>	
	</insert>
	
</mapper>